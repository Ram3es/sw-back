    "@nestjs/platform-express": "^9.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "db-migrate": "^0.11.13",
    "db-migrate-mysql": "^2.2.0",
    "express-session": "^1.17.3",
    "mysql2": "^3.4.2",
    "nestjs-pino": "^3.3.0",
    "passport": "^0.6.0",
    "passport-steam": "^1.0.18",
    "pino": "^8.14.1",
